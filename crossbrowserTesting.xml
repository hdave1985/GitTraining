<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Opencart Test Suite" parallel="tests" Thread-count="2">

<!-- Add parallel="tests" tag to execute the test parallely on different browsers -->

<test name="Chrome test cases ">
<parameter name="browser" value="chrome"/>
	<classes>
		<class name="testCases.TC_Login"></class>
	</classes>
</test>

<test name="Edge test cases ">
<parameter name="browser" value="edge"/>
	<classes>
		<class name="testCases.TC_Login"></class>
	</classes>
</test>

<test name="Firefox  test cases ">
<parameter name="browser" value="firefox"/>
	<classes>
		<class name="testCases.TC_Login"></class>
	</classes>
</test>


</suite>